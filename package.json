{
    "name": "nodejs",
    "version": "1.0.0",
    "description": "boilerplate for a node js app",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "test": "mocha --recursive --timeout 700000 --exit",
        "start": "node src/server.js",
        "start-server": "node src/server.js",
        "dev": "nodemon src/server.js",
        "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
        "prettier": "npx prettier --write .",
        "lint": "eslint",
        "lint:fix": "eslint . --fix",
        "git_push": "node -e \"const mssg = ' ' + process.argv[1]; require('child_process').execSync('npx prettier --write . && git add . && git commit -m \\\"' + mssg + '\\\" && git push -u origin master', { stdio:[0, 1, 2] } ) \"",
        "git_push_dev": "node -e \"const mssg = ' ' + process.argv[1]; require('child_process').execSync('npx prettier --write . && git add . && git commit -m \\\"' + mssg + '\\\" && git push -u origin development', { stdio:[0, 1, 2] } ) \""
    },
    "author": "Iwatannaye",
    "license": "ISC",
    "dependencies": {
        "@sendgrid/mail": "^7.4.4",
        "aws-sdk": "^2.865.0",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "celebrate": "^13.0.4",
        "chai": "^4.3.4",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cryptr": "^6.0.2",
        "dayjs": "^1.10.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mocha": "^9.1.1",
        "mockgoose": "^8.0.4",
        "mongodb": "^3.6.5",
        "mongoose": "^5.12.0",
        "mongoose-paginate-v2": "^1.3.17",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "node-cron": "^3.0.0",
        "nodemon": "^2.0.7",
        "uniqid": "^5.3.0",
        "validatorjs": "^3.22.1",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.1.2",
        "prettier-eslint": "^13.0.0"
    }
}
